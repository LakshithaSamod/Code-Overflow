#include <iostream>  
using namespace std;  
int main()  
{  
int no,r,sum=0;
int temp;    
cout<<"Enter Number";    
cin>>no;    
temp=no;    
while(no>0)    
{    
r=no%10;    
sum=sum+(r*r*r);    
no=no/10;    
}    
if(temp==sum)    
cout<<"Given number is an Armstrong Number."<<endl;    
else    
cout<<"Given number is not Armstrong Number."<<endl;   
return 0;  
}  
